{
	"name": "fa-node",
	"version": "0.1.0",
	"description": "",
	"author": "Ivan Kosenko",
	"license": "MIT",
	"private": false,
	"homepage": "https://github.com/foxart/fa-node",
	"scripts": {
		"start:console": "nodemon -e ts --watch ../ -x ts-node src/examples/console.ts",
		"start:crypt": "nodemon -e ts --watch ../ -x ts-node src/examples/crypt.ts",
		"start:data": "nodemon -e ts --watch ../ -x ts-node src/examples/data.ts",
		"start:param-decorator": "nodemon -e ts --watch ../ -x ts-node src/examples/param-decorator.ts",
		"start:parser": "nodemon -e ts --watch ../ -x ts-node src/examples/parser.ts",
		"start:system": "nodemon -e ts --watch ../ -x ts-node src/examples/system.ts",
		"start:validator": "nodemon -e ts --watch ../ -x ts-node src/examples/validator.ts",
		"build": "rm -rf ./lib && npm run build:esm && npm run build:cjs",
		"build:esm": "tsc -p config/tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
		"build:cjs": "tsc -p config/tsconfig.cjs.json",
		"link": "npm link",
		"prepack": "npm run build",
		"export": "npm pack --pack-destination packages --omit=dev",
		"publish:public": "npm publish --access=public",
		"publish:dry": "npm publish --dry-run",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"main": "./lib/cjs/index.js",
	"types": "./lib/cjs/types/index.d.ts",
	"files": [
		"lib/**/*"
	],
	"exports": {
		".": {
			"import": {
				"types": "./lib/esm/types/index.d.ts",
				"default": "./lib/esm/index.mjs"
			},
			"require": {
				"types": "./lib/cjs/types/index.d.ts",
				"default": "./lib/cjs/index.js"
			}
		}
	},
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
		"crypto-js": "^4.1.1",
		"reflect-metadata": "^0.1.13",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@types/bcryptjs": "^2.4.6",
		"@types/crypto-js": "^4.2.1",
		"@types/jest": "^29.5.3",
		"@types/node": "^20.4.9",
		"@types/uuid": "^9.0.2",
		"@typescript-eslint/eslint-plugin": "^5.0.0",
		"@typescript-eslint/parser": "^5.0.0",
		"eslint": "^8.0.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.2.1",
		"jest": "^29.6.2",
		"nodemon": "^3.0.1",
		"prettier": "^2.7.1",
		"source-map-support": "^0.5.21",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.1"
	}
}
