{
	"name": "fa-node",
	"version": "0.1.0",
	"description": "",
	"author": "Ivan Kosenko",
	"license": "MIT",
	"private": false,
	"homepage": "https://github.com/foxart/fa-node",
	"scripts": {
    "start:console": "nodemon -e ts --watch ../ -x ts-node examples/test-console-helper.ts",
    "start:crypt": "nodemon -e ts --watch ../ -x ts-node examples/test-crypt-helper.ts",
    "start:data": "nodemon -e ts --watch ../ -x ts-node examples/data.ts",
    "start:param-decorator": "nodemon -e ts --watch ../ -x ts-node examples/test-param-decorator.helper.ts",
    "start:parser": "nodemon -e ts --watch ../ -x ts-node examples/parser.ts",
    "start:system": "nodemon -e ts --watch ../ -x ts-node examples/test-system-helper.ts",
    "start:validator": "nodemon -e ts --watch ../ -x ts-node examples/test-validator-helper.ts",
    "build": "rm -rf ./lib && npm run build:esm && npm run build:cjs",
    "build_": "npm run build:dist",
    "prebuild": "rimraf dist",
    "build:dist": "tsc -p tsconfig.dist.json",
    "build:esm": "tsc -p tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "lint": "eslint -c .eslintrc.js \"{src,apps,libs,test}/**/*.ts\" --fix",
		"prepack": "npm run build",
		"publish:public": "npm publish --access=public",
		"publish:dry": "npm publish --dry-run",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"main": "./lib/cjs/index.js",
	"types": "./lib/cjs/types/index.d.ts",
	"files": [
		"lib/**/*"
	],
	"exports": {
		".": {
			"import": {
				"types": "./lib/esm/types/index.d.ts",
				"default": "./lib/esm/index.mjs"
			},
			"require": {
				"types": "./lib/cjs/types/index.d.ts",
				"default": "./lib/cjs/index.js"
			}
		}
	},
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "crypto-js": "^4.2.0",
    "reflect-metadata": "^0.2.1",
    "uuid": "^9.0.1"
	},
	"devDependencies": {
		"@types/bcryptjs": "^2.4.6",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.6.4",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-multiline-arrays": "^3.0.4",
    "rimraf": "^5.0.5",
		"source-map-support": "^0.5.21",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
	}
}
